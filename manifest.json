{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "permissions": [
          "storage"
  ],
  "browser_action": {
    "default_icon": "ucsc.jpg",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css https://code.jquery.com/jquery-1.10.2.js https://code.jquery.com/ui/1.10.4/jquery-ui.js; object-src 'self'",
 
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["content.js"]
    }
  ]  

}
 
  

