{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "permissions": [
          "storage"
  ],
  "browser_action": {
    "default_icon": "ucsc.jpg",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["content.js"]
    }
  ]  

}
 
  

