{
  "manifest_version": 2,
  "name": "tabspace",
  "version": "0.1",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["popup.js"],
        "css": ["style.css"]
    }
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"

}
 
  

