{
  "manifest_version": 2,
  "name": "tabspace",
  "version": "0.1",
  "permissions": [
          "storage"
  ],
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup/popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["new_tab.js"]
    }
  ]  
}
 
  

